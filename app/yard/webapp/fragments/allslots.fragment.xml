
 
<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:l="sap.ui.layout">
 <Popover
        id="idallslotsDialog"
        resizable="true"
       titleAlignment="Center"
        placement ="PreferredLeftOrFlip"
        class="popOverClass"
    >
    <customHeader>
            <Bar>
            <contentLeft>
           
            </contentLeft>
                <contentRight>
                    <Button
                        id="allslotscancelbtn"
                        icon="sap-icon://sys-cancel"
                        press="onClose2"
                        type="Default"
                    />
                </contentRight>
            </Bar>
        </customHeader>
       
       <f:Card>
        <f:content>
              <List
              class="sapUiContentPadding"
                    id="idallslotsh1"
                   
                    backgroundDesign="Transparent"
                     items="{
            path: '/PlotNOs'
           
        }"
                    growing="true"
                   
                    growingThreshold="5"
                >
                    <items >
                        <CustomListItem  id="idColumnListBooksLoan">
                            <FlexBox justifyContent="SpaceAround">
                    <items>
                        <Text text="{plot_NO}"/>
                       
                    </items>
                </FlexBox>
                           
                        </CustomListItem>
                    </items>
                </List>
 
               
        </f:content>
    </f:Card>
 
         </Popover>
           
</core:FragmentDefinition>
 
 
 